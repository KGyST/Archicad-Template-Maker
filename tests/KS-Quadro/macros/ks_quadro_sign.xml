!!			-0.413397,	0.06,	15,
!!			0.413397,	0.06,	15,
!!			0,	0.776025,	15,
!!			-0.413397,	0.06,	-1

		n=REQUEST("Pen_of_RGB", "0 0 0", penDrawing)
		define material "Sign_black" 2, 0/255, 0/255, 0/255
		material "Sign_black"
		pen penDrawing

		prism_ 13, EPS,
			-0.0395646,	0.568345,	79,
			-0.0395646,	0.548345,	900,
			0,	95.2941,	4079,
			-0.0288806,	0.216289,	79,
			-0.00896591,	0.218135,	900,
			0,	84.7059,	4079,
			0.00896591,	0.198135,	79,
			0.00896591,	0.218135,	900,
			0,	84.7059,	4079,
			0.0594793,	0.5465,	79,
			0.0395646,	0.548345,	900,
			0,	95.2941,	4079,
			-0.0395646,	0.568345,	-1

		prism_ 7, EPS,
			0.03,	0.129866,	79,
			0,	0.129866,	900,
			0,	180,	4079,
			-0.03,	0.129866,	79,
			0,	0.129866,	900,
			0,	180,	4079,
			0.03,	0.129866,	-1

		n=REQUEST("Pen_of_RGB", "255 0 0", penDrawing)
		pen penDrawing

		poly2_b 14, 3, penDrawing, penDrawing,
			-0.49134,	0.045,	1,
			-0.465359,	0.03,	900,
			0,	120,	4001,
			0.465359,	0,	1,
			0.465359,	0.03,	900,
			0,	120,	4001,
			0.0259808,	0.851025,	1,
			-1.06581e-014,	0.836025,	900,
			0,	120,	4001,
			-0.49134,	0.045,	-1,
			0.413397,	0.06,	1,
			-0.413397,	0.06,	1,
			0,	0.776025,	1,
			0.413397,	0.06,	-1

	del 1
return


! -----------------------------------------------------------------------
2:		! Collision Sign
! -----------------------------------------------------------------------
	wTriangle		= B
	distTriangle	= B/5

	n=REQUEST("Pen_of_RGB", "255 0 0", penDrawing)
	define material "Sign_red_C" 2, 255/255, 0/255, 0/255
	material "Sign_red"
	pen penDrawing

	rotx 90
	addz -wTriangle
		for i = 0 to 1
			roty 180 * i
				plane 3, 
					-wTriangle / 2, 0, -wTriangle,
					wTriangle / 2 - distTriangle, 0, wTriangle,
					-wTriangle / 2, 0, wTriangle
			del 1
		next i
	del 2
return
]]>

! -----------------------------------------------------------------------
1:		! WarnigSign
! -----------------------------------------------------------------------
	mul A/0.991, B/0.866, 1
		n=REQUEST("Pen_of_RGB", "255 0 0", penDrawing)
		define material "Sign_red" 2, 255/255, 0/255, 0/255
		material "Sign_red"
		pen penDrawing

		prism_ 14, EPS,
			-0.49134,	0.045,	79,
			-0.465359,	0.03,	900,
			0,	120,	4079,
			0.465359,	0,	79,
			0.465359,	0.03,	900,
			0,	120,	4079,
			0.0259808,	0.851025,	79,
			-1.06581e-014,	0.836025,	900,
			0,	120,	4079,
			-0.49134,	0.045,	-1,
			0.413397,	0.06,	15,
			-0.413397,	0.06,	15,
			0,	0.776025,	15,
			0.413397,	0.06,	-1

